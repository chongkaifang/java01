<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.offcn.dao.MobileMapper">
	<insert id="insert" parameterType="com.offcn.po.Mobile">
		insert into from mobile(MobileNumber,MobileArea,MobileType,AreaCode,PostCode) 
		values(#{MobileNumber},#{MobileArea},#{MobileType},#{AreaCode},#{PostCode})
	</insert>
</mapper>